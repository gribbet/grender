<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title>Grender Jersey Example</title>
        <meta charset="UTF-8"/>
        <link rel="stylesheet"
              href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.1.1/css/bootstrap.no-responsive.no-icons.min.css"/>
        <link rel="stylesheet" href="main.css"/>
    </head>

    <body>
        <div class="container">
            <div class="row">
                <div class="span9">
                    <div class="body">
                        <h1>View Contact</h1>
                        <div class="form-horizontal">
                            <div class="control-group">
                                <label class="control-label">Name</label>
                                <div class="controls">
                                    <span class="name value"/>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Address</label>
                                <div class="controls">
                                    <span class="address value"/>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">City</label>
                                <div class="controls">
                                    <span class="city value"/>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">State</label>
                                <div class="controls">
                                    <span class="state value"/>
                                </div>
                            </div>
                            <div class="control-group">
                                <label class="control-label">Zip</label>
                                <div class="controls">
                                    <span class="zip value"/>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </body>
</html>