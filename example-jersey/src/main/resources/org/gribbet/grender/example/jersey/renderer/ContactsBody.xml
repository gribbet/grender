<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
    <head>
        <title>Grender Jersey Example</title>
        <meta charset="UTF-8"/>
        <link rel="stylesheet"
              href="//netdna.bootstrapcdn.com/twitter-bootstrap/2.1.1/css/bootstrap.no-responsive.no-icons.min.css"/>
        <link rel="stylesheet" href="main.css"/>
    </head>

    <body>
        <div class="container">
            <div class="row">
                <div class="span9">
                    <div class="body">
                        <h1>Contacts</h1>
                        <table class="contacts table">
                            <thead>
                                <tr>
                                    <th>Name</th>
                                    <th>Address</th>
                                    <th>City</th>
                                    <th>State</th>
                                    <th>Zip</th>
                                    <th>Action</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr>
                                    <td class="name"/>
                                    <td class="address"/>
                                    <td class="city"/>
                                    <td class="state"/>
                                    <td class="zip"/>
                                    <td class="action">
                                        <a class="view">View</a>
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </body>
</html>